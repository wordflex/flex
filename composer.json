{
    "name": "wordflex/flex",
    "type": "composer-plugin",
    "description": "Composer plugin for Wordflex",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien.potencier@gmail.com"
        },
        {
            "name": "Bao Tran",
            "email": "baotd.weebpal@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.0.2",
        "symfony/dotenv": "^3.4|^4.0|^5.0",
        "symfony/phpunit-bridge": "^3.4.19|^4.1.8|^5.0",
        "symfony/process": "^2.7|^3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Wordflex\\Flex\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        },
        "class": "Wordflex\\Flex\\Flex"
    }
}
